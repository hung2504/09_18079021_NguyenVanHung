<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".CustomerScreen"
    android:background="#214F4F">
    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="River Restaurance"
        android:textColor="#FFD233"
        android:textSize="30dp"
        android:textStyle="bold"
        android:layout_marginLeft="15dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="48dp"
        app:layout_constraintBottom_toBottomOf="@+id/textView3"
        app:layout_constraintStart_toEndOf="@+id/textView3"
        app:layout_constraintTop_toTopOf="@+id/textView3"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/tomi" />
    <TextView
        android:id="@+id/tvLine"
        android:layout_width="match_parent"
        android:layout_height="3px"
        android:background="@drawable/linecus"
        android:layout_marginTop="5dp"
        app:layout_constraintTop_toBottomOf="@id/imageView2">

    </TextView>
    <ImageButton
        android:id="@+id/banEmp1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/ban"
        app:layout_constraintTop_toBottomOf="@id/tvLine"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="40dp"
        android:layout_marginLeft="50dp"></ImageButton>
    <TextView
        android:id="@+id/tvbanEmp1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Bàn 01"
        android:textColor="@color/white"
        android:textSize="20sp"
        app:layout_constraintTop_toBottomOf="@id/banEmp1"
        app:layout_constraintStart_toStartOf="@id/banEmp1"
        app:layout_constraintEnd_toEndOf="@id/banEmp1"
        android:layout_marginTop="5dp"></TextView>
    <ImageButton
        android:id="@+id/banEmp2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/ban"
        app:layout_constraintTop_toBottomOf="@id/tvLine"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="40dp"
        android:layout_marginRight="50dp"></ImageButton>
    <TextView
        android:id="@+id/tvbanEmp2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Bàn 02"
        android:textColor="@color/white"
        android:textSize="20sp"
        app:layout_constraintTop_toBottomOf="@id/banEmp2"
        app:layout_constraintStart_toStartOf="@id/banEmp2"
        app:layout_constraintEnd_toEndOf="@id/banEmp2"
        android:layout_marginTop="5dp"></TextView>
    <ImageButton
        android:id="@+id/banEmp3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/ban"
        app:layout_constraintTop_toBottomOf="@id/tvbanEmp1"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="80dp"
        android:layout_marginLeft="50dp"></ImageButton>
    <TextView
        android:id="@+id/tvbanEmp3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Bàn 03"
        android:textColor="@color/white"
        android:textSize="20sp"
        app:layout_constraintTop_toBottomOf="@id/banEmp3"
        app:layout_constraintStart_toStartOf="@id/banEmp3"
        app:layout_constraintEnd_toEndOf="@id/banEmp3"
        android:layout_marginTop="5dp"></TextView>
    <ImageButton
        android:id="@+id/banEmp4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/ban"
        app:layout_constraintTop_toBottomOf="@id/tvbanEmp2"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="80dp"
        android:layout_marginRight="50dp"></ImageButton>
    <TextView
        android:id="@+id/tvbanEmp4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Bàn 04"
        android:textColor="@color/white"
        android:textSize="20sp"
        app:layout_constraintTop_toBottomOf="@id/banEmp4"
        app:layout_constraintStart_toStartOf="@id/banEmp4"
        app:layout_constraintEnd_toEndOf="@id/banEmp4"
        android:layout_marginTop="5dp"></TextView>
    <ImageButton
        android:id="@+id/banEmp5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/ban"
        app:layout_constraintTop_toBottomOf="@id/tvbanEmp3"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="80dp"
        android:layout_marginLeft="50dp"></ImageButton>
    <TextView
        android:id="@+id/tvbanEmp5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Bàn 05"
        android:textColor="@color/white"
        android:textSize="20sp"
        app:layout_constraintTop_toBottomOf="@id/banEmp5"
        app:layout_constraintStart_toStartOf="@id/banEmp5"
        app:layout_constraintEnd_toEndOf="@id/banEmp5"
        android:layout_marginTop="5dp"></TextView>
    <ImageButton
        android:id="@+id/banEmp6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/ban"
        app:layout_constraintTop_toBottomOf="@id/tvbanEmp4"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="80dp"
        android:layout_marginRight="50dp"></ImageButton>
    <TextView
        android:id="@+id/tvbanEmp6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Bàn 06"
        android:textColor="@color/white"
        android:textSize="20sp"
        app:layout_constraintTop_toBottomOf="@id/banEmp6"
        app:layout_constraintStart_toStartOf="@id/banEmp6"
        app:layout_constraintEnd_toEndOf="@id/banEmp6"
        android:layout_marginTop="5dp"></TextView>
    <TextView
        android:id="@+id/tvLine1"
        android:layout_width="match_parent"
        android:layout_height="3px"
        android:background="@drawable/linecus"
        android:layout_marginTop="60dp"
        app:layout_constraintTop_toBottomOf="@id/tvbanEmp5"
        ></TextView>

</androidx.constraintlayout.widget.ConstraintLayout>